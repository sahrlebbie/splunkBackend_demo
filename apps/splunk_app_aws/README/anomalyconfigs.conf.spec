# This file contains the detail information of anomaly detection jobs.

[<UUID of job>]
job_name = <string>
* Name of job

job_description = <string>
* Description of job

job_priority = <string>
* Priority of job

job_schedule = <string>
* Schedule type of job, for now, it could be Hourly, Daily, Weekly and Monthly

job_tags = <string>
* Tags of job, split by ","

job_train = <string>
* Train period of job

job_search = <string>
* Search SPL of job

job_alertId = <string>
* Bound alert's ID

job_mode = <string>
* Mode of job
